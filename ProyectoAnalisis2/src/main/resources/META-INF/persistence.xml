<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="conexion" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.analisis2.clases.modelo.Detallefacturacompra</class>
    <class>com.analisis2.clases.modelo.Producto</class>
    <class>com.analisis2.clases.modelo.Proveedores</class>
    <class>com.analisis2.clases.modelo.Cuenta</class>
    <class>com.analisis2.clases.modelo.Arreglo</class>
    <class>com.analisis2.clases.modelo.ArregloHasProducto</class>
    <class>com.analisis2.clases.modelo.Facturacompra</class>
    <class>com.analisis2.clases.modelo.Marca</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/proyectoanalisis?useUnicode=true&amp;useJDBCCompliantTimezoneShift=true&amp;useLegacyDatetimeCode=false&amp;serverTimezone=UTC"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="1234"/>
    </properties>
  </persistence-unit>
</persistence>
